public class BasicTest {
	Logic logic;
@Before
public void hello()
{
 logic = new Logic();
}


@Test	
public void forgotReplyTest()
{
	User u = new User();
	u.setUname("keerthan");

	assertEquals("Hi"+u.getUname().toUpperCase()+"! Password has been sent to your registered Mail.",logic.forgetReply(u));

	
}



}

